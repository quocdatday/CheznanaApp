        <div class="container-fluid py-3 py-lg-0">
            <a class="navbar-brand d-lg-none offcanvas-logo" asp-page="#">
                <h1 class="m-0 ps-2">Cheznana</h1>
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasNavbar" aria-controls="offcanvasNavbar" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="offcanvas offcanvas-end" tabindex="-1" id="offcanvasNavbar" aria-labelledby="offcanvasNavbarLabel">
                <div class="offcanvas-body">
                    @{
                        var currentPage = ViewContext.RouteData.Values["page"]?.ToString()?.ToLower();
                        // var currentArea = ViewContext.RouteData.Values["area"]?.ToString()?.ToLower();
                    }
                    <ul class="navbar-nav mx-auto justify-content-end flex-grow-1 pe-3">
                        <li class="nav-item">
                            <a class="nav-link navbar-text @(currentPage!.StartsWith("/index") ? "active" : "")" asp-page="/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link navbar-text @(currentPage!.StartsWith("/privacy") ? "active" : "")" asp-page="/Privacy">Về Cheznana</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link navbar-text @(currentPage!.StartsWith("/collection") ? "active" : "")" asp-page="/Collection">Bộ sưu tập</a>
                        </li>
                    </ul>
                    <div class="nav-logo d-none d-lg-block" style="width:180px;">
                        <a asp-page="/Index">
                            <img class="backgroundImg" src="~/img/backgroundLogo.png" alt="BackgroundLogo" style="width: 180px; height: 180px;" />
                            <img class="logoImg" src="~/img/logonavCheznana.png" alt="Cheznana" style="width: 180px; height: 180px;" />
                        </a>
                    </div>
                    <ul class="navbar-nav mx-auto justify-content-start flex-grow-1 pe-3">
                        <li class="nav-item">
                            <a class="nav-link navbar-text" asp-page="/Service">Dịch vụ B2B</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link navbar-text @(currentPage!.StartsWith("/faq") ? "active" : "")" asp-page="/Faq">FAQ</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link navbar-text" asp-page="/Contact">Liên hệ</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <script>
            const nav = document.querySelector('nav');
            const navHeight = nav.getBoundingClientRect().height;
            const navwave = document.querySelector('.nav-wave');
            navwave.style.bottom = (-navHeight + 15) + 'px';
        </script>
    </nav>


    <!--content start-->
    <div id="content" class="content">
        @RenderBody()
    </div>
    <!--content end-->


